.PHONY: clean All

All:
	@echo "----------Building project:[ Section_10_CppStyleStringExercise - Debug ]----------"
	@cd "Section_10_CppStyleStringExercise" && "$(MAKE)" -f  "Section_10_CppStyleStringExercise.mk"
clean:
	@echo "----------Cleaning project:[ Section_10_CppStyleStringExercise - Debug ]----------"
	@cd "Section_10_CppStyleStringExercise" && "$(MAKE)" -f  "Section_10_CppStyleStringExercise.mk" clean
