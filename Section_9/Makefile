.PHONY: clean All

All:
	@echo "----------Building project:[ Section_9_Challenge - Debug ]----------"
	@cd "Section_9_Challenge" && "$(MAKE)" -f  "Section_9_Challenge.mk"
clean:
	@echo "----------Cleaning project:[ Section_9_Challenge - Debug ]----------"
	@cd "Section_9_Challenge" && "$(MAKE)" -f  "Section_9_Challenge.mk" clean
