.PHONY: clean All

All:
	@echo "----------Building project:[ RangeBasedForLoop - Debug ]----------"
	@cd "RangeBasedForLoop" && "$(MAKE)" -f  "RangeBasedForLoop.mk"
clean:
	@echo "----------Cleaning project:[ RangeBasedForLoop - Debug ]----------"
	@cd "RangeBasedForLoop" && "$(MAKE)" -f  "RangeBasedForLoop.mk" clean
