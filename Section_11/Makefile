.PHONY: clean All

All:
	@echo "----------Building project:[ FunctionsIntro - Debug ]----------"
	@cd "FunctionsIntro" && "$(MAKE)" -f  "FunctionsIntro.mk"
clean:
	@echo "----------Cleaning project:[ FunctionsIntro - Debug ]----------"
	@cd "FunctionsIntro" && "$(MAKE)" -f  "FunctionsIntro.mk" clean
