.PHONY: clean All

All:
	@echo "----------Building project:[ Exercise_25_Passing_Arrays_to_Functions - Debug ]----------"
	@cd "Exercise_25_Passing_Arrays_to_Functions" && "$(MAKE)" -f  "Exercise_25_Passing_Arrays_to_Functions.mk"
clean:
	@echo "----------Cleaning project:[ Exercise_25_Passing_Arrays_to_Functions - Debug ]----------"
	@cd "Exercise_25_Passing_Arrays_to_Functions" && "$(MAKE)" -f  "Exercise_25_Passing_Arrays_to_Functions.mk" clean
