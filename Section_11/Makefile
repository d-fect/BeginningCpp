.PHONY: clean All

All:
	@echo "----------Building project:[ 109_Pass_by_Reference - Debug ]----------"
	@cd "109_Pass_by_Reference" && "$(MAKE)" -f  "109_Pass_by_Reference.mk"
clean:
	@echo "----------Cleaning project:[ 109_Pass_by_Reference - Debug ]----------"
	@cd "109_Pass_by_Reference" && "$(MAKE)" -f  "109_Pass_by_Reference.mk" clean
